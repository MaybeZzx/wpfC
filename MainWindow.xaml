<Window x:Class="employee.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:employee"
        mc:Ignorable="d"
        Title="MainWindow" Height="292" Width="183" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="buttonTask1" Content="ListEmployee" Margin="11,10,11,0" VerticalAlignment="Top" Height="35" FontSize="16" Click="buttonTask1_Click"/>
        <Button x:Name="buttonTask2" Content="AddEmployee" Margin="11,50,11,0" VerticalAlignment="Top" Height="35" FontSize="16" Click="buttonTask2_Click"/>
        <Button x:Name="buttonTask3" Content="ReadInfo" Margin="11,90,11,0" VerticalAlignment="Top" Height="35" FontSize="16" Click="buttonTask3_Click"/>
        <Button x:Name="buttonTask4" Content="DeleteInfo" Margin="10,130,12,0" VerticalAlignment="Top" Height="35" FontSize="16" Click="buttonTask4_Click"/>
        <Button x:Name="buttonTask5" Content="DepartmentInfo" Margin="10,170,12,0" VerticalAlignment="Top" Height="35" FontSize="16" Click="buttonTask5_Click"/>
        <Button x:Name="buttonTask6" Content="ManageEmployee" Margin="10,210,12,0" VerticalAlignment="Top" Height="35" FontSize="16" Click="buttonTask6_Click"/>

    </Grid>
</Window>
